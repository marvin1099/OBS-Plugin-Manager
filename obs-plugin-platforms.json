{
    "url-match": [
        {
            "has": "**macos*.zip",
            "os": "macos",
            "return": true
        },
        {
            "has": "**darwin*.zip",
            "os": "macos",
            "return": true
        },
        {
            "has": "**linux*.zip",
            "os": "linux",
            "return": true
        },
        {
            "has": "**.zip",
            "os": "windows",
            "return": true
        },
        {
            "overwrite": "https://api.%0/repos/%1/%2/releases/latest",
            "type": "releases",
            "follow": true
        },
        {
            "overwrite": "https://api.%0/repos/%1/%2",
            "type": "repo",
            "follow": true
        }
    ],
    "pages": {
        "github.com": {
            "url-match": []
        }
    }
}